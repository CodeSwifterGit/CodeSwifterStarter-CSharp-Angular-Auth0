{
  "name": "codeswifterstarter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve-production": "ng serve --configuration=\"production\"",
    "serve-staging": "ng serve --configuration=\"staging\"",
    "build-staging": "ng build --configuration=\"staging\"",
    "build-prod": "ng build --configuration=\"production\"",
    "build:ssr-prod": "npm run build:client-and-server-bundles-prod && npm run webpack:server",
    "serve:ssr-prod": "node dist/server.js",
    "build:client-and-server-bundles-prod": "ng build --configuration=\"production\" && ng build --configuration=\"production\" --app 1 --output-hashing=false",
    "build:ssr-staging": "npm run build:client-and-server-bundles-staging && npm run webpack:server",
    "serve:ssr-staging": "node dist/server.js",
    "build:client-and-server-bundles-staging": "ng build --configuration=\"staging\" && ng build --configuration=\"staging\" --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc — properties es2015 browser module main — first-only — create-ivy-entry-point — async false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.7",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@auth0/auth0-spa-js": "^1.13.5",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-split": "^5.0.0",
    "lodash-es": "^4.17.20",
    "ngx-scrollbar": "^7.5.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
